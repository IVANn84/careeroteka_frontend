{
  "name": "careeroteka",
  "version": "1.0.0",
  "description": "Энциклопедия о карьере с данными от реальных специалистов и экспертов",
  "author": "TroExol",
  "main": "./src/index.js",
  "dependencies": {
    "@heroicons/react": "2.0.12",
    "accounting-big": "1.0.1",
    "axios": "0.25.0",
    "axios-case-converter": "0.9.0",
    "big.js": "6.2.1",
    "chart.js": "3.9.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "clone": "2.1.2",
    "jss": "10.9.2",
    "jss-plugin-default-unit": "10.9.2",
    "jss-plugin-vendor-prefixer": "10.9.2",
    "mobx": "6.7.0",
    "mobx-react-lite": "3.4.0",
    "mobx-state-tree": "5.1.6",
    "moment": "2.29.4",
    "query-string": "7.1.1",
    "react": "18.2.0",
    "react-chartjs-2": "4.3.1",
    "react-content-loader": "6.2.0",
    "react-dom": "18.2.0",
    "react-infinite-scroll-component": "6.1.0",
    "react-jss": "10.9.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-sticky-el": "2.0.9"
  },
  "devDependencies": {
    "@babel/core": "7.10.0",
    "@babel/plugin-proposal-decorators": "7.10.0",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.10.0",
    "@babel/preset-react": "7.10.0",
    "@babel/register": "7.9.0",
    "autoprefixer": "7.1.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-require-ignore": "0.1.1",
    "babel-plugin-webpack-alias": "2.1.2",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "clean-webpack-plugin": "2.0.2",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-plugin-react": "7.18.0",
    "favicons": "7.0.1",
    "favicons-webpack-plugin": "4.2.0",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "4.5.2",
    "minimist": "1.2.6",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-mixins": "6.2.3",
    "postcss-nested": "4.2.1",
    "postcss-simple-vars": "5.0.2",
    "postcss-url": "8.0.0",
    "prop-types": "15.8.1",
    "robotstxt-webpack-plugin": "7.0.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "2.3.7",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2"
  },
  "scripts": {
    "postinstall": "git config core.hooksPath ./git-hooks",
    "local": "webpack-dev-server --watch --progress --mode=development --inline --hot --config webpack/local.js",
    "production": "webpack --mode=production --config webpack/production.js",
    "linter": "eslint src/ --ext .js --ext .jsx",
    "linter:pages": "eslint src/pages --ext .js --ext .jsx",
    "linter:components": "eslint src/components --ext .js --ext .jsx"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "decoratorsBeforeExport": true
        }
      ],
      [
        "babel-plugin-transform-require-ignore",
        {
          "extensions": [
            ".css"
          ]
        }
      ],
      [
        "module-resolver",
        {
          "alias": {
            "^Util(.*)": "./src/utils\\1",
            "^Page(.*)": "./src/pages\\1",
            "^Component(.*)": "./src/components\\1",
            "^Image": "./src/images\\1",
            "^Api": "./src/api/methods\\1",
            "^Mock": "./src/api/mock\\1",
            "^Hook": "./src/hooks\\1",
            "^Hoc": "./src/hoc\\1",
            "^Theme": "./src/themes\\1"
          }
        }
      ]
    ]
  }
}
